<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.identity.activity.PushSettingActivity">

    <include android:id="@+id/push_setting_message_include"
        layout="@layout/include_push_setting"/>

    <TextView android:id="@+id/push_setting_space"
        android:layout_width="match_parent"
        android:layout_height="14dp"
        android:layout_below="@id/push_setting_message_include"/>

    <include android:id="@+id/push_setting_night_quietness_include"
        layout="@layout/include_push_setting"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_below="@id/push_setting_space"/>





</RelativeLayout>